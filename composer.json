{
  "name": "riskivy/yii2-export",
  "description": "A Yii2 wrapper component for the mPDF、excel、docs library which generates PDF files from UTF-8 encoded HTML.",
  "keywords": [
    "yii2",
    "extension",
    "pdf",
    "excel",
    "docs",
    "mpdf",
    "utf8",
    "html",
    "component"
  ],
  "homepage": "https://github.com/riskivy/yii2-export",
  "type": "yii2-extension",
  "license": "BSD 3-Clause",
  "authors": [
    {
      "name": "Riskivy Visweswaran",
      "email": "jack.hu@tophant.com",
      "homepage": "https://www.tophant.com/"
    }
  ],
  "require": {
    "mpdf/mpdf": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "riskivy\\export\\pdf\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
